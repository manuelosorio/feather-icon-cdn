{
  "name": "icon-feathers-cdn",
  "version": "1.2.1",
  "engines": {
    "node": ">=22.0.0"
  },
  "description": "",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently -k -n 'Tsup,Node' -c 'yellow.bold,cyan.bold' 'npm run watch:tsup' 'nodemon dist/server.js'",
    "watch:tsup": "tsup src/server.ts --watch --onSuccess 'npm run copy-files'",
    "build": "tsup && npm run copy-files",
    "clean": "rimraf ./dist",
    "test": "cypress open",
    "copy-files": "copyfiles -u 1 './src/views/template/*.html' './src/views/template/*.pug' ./dist",
    "format": "prettier --write ."
  },
  "author": "Manuel Osorio",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express-flash": "^0.0.5",
    "@types/express-session": "^1.18.1",
    "@types/feather-icons": "^4.29.4",
    "@types/helmet": "^4.0.0",
    "@types/node": "^22.15.21",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "copy-and-watch": "^0.1.8",
    "copyfiles": "^2.4.1",
    "cypress": "^14.4.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-prettier": "^5.4.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "body-parser": "^2.2.0",
    "chalk": "^5.4.1",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-flash": "^0.0.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "feather-icons": "^4.29.2",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "posthog-node": "^4.17.2",
    "pug": "^3.0.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
